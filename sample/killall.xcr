use base qw(core);

%xyz = (
    'id' => 'job100',
    'exe' => 'echo',
    'queue' => 'gh10034',
    'option' => '# @$-g gh10034'
);

my @jobs = &prepare(%xyz, 'arg0S' => [10],
 'after' => 'if ($self->{stdout} == 1) { system("qdel -u $ENV{USERNAME}"); }'
);
my @thrds = &submit(@jobs);
my @results  = &sync(@thrds);
foreach (@results) { print $_->{stdout} , "\n"; }
